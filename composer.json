{
    "name": "lm-commons/lmc-bootstrap-menu",
    "description": "This repository can generate nested bootstrap menu based on ul li tag. More may come later. Thanks!",
    "keywords": [
        "laminas",
        "bootstrap",
        "lm-commons",
        "LmcBootstrapMenu"
    ],
    "authors":[
      {
        "name":"Shah Mubashir Hussain",
        "email":"shah.mubashir@gmail.com",
        "homepage": "http://www.lrphpt.com"
      }
    ],
    "support": {
        "issues": "https://github.com/LM-Commons/LmcBootstrapMenu/issues",
        "source": "https://github.com/LM-Commons/LmcBootstrapMenu",
        "rss": "https://github.com/LM-Commons/LmcBootstrapMenu/releases.atom"
    },
    "extra": {
        "lm": {
            "component": "LmcBootstrapMenu",
            "config-provider": [
                "LmcBootstrapMenu\\ConfigProvider"
            ]
        }
    },
    "require": {
        "php" : "^7.3 || ^8.0",
        "laminas/laminas-modulemanager" : "^2.5",
        "laminas/laminas-loader" : "^2.6",
        "laminas/laminas-eventmanager" : "^3.0",
        "laminas/laminas-mvc" : "^3.0",
        "laminas/laminas-navigation" : "^2.10"
    },
    "autoload": {
        "psr-4": {
            "LmcBootstrapMenu\\": "src/"
        }
    }
}
